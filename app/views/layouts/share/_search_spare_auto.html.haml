.wrap_search_parts_auto
  .search_search_parts_auto
    %h4 Поиск:
    %ul.nav.nav-tabs
      %li
        %a{"data-toggle" => "tab", href: "#spares_search"} По номеру
      %li
        %a{"data-toggle" => "tab", href: "#auto_search"} По машинам
    
  .tab-content
    #spares_search.tab-pane.active
      = search_form_for @search_spares, url: search_spare_parts_path, html: {class: "form-search",  method: :post } do |f|
        .field
          //= f.label :title_cont
          = f.text_field :title_cont, class: "input-medium search-query"
        .form-actions
          = f.submit "search", class: "btn btn-small"

    #auto_search.tab-pane ...
    